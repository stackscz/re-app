{
  "name": "re-app",
  "version": "0.22.0-dev.31",
  "description": "react + redux frontend codebase",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/stackscz/re-app"
  },
  "engines": {
    "node": "^4.0.0",
    "npm": "^3.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "ab": "node ./node_modules/re-app-builder",
    "clean": "rimraf lib/",
    "build": "npm run clean && node ./node_modules/babel-cli/bin/babel src/ --out-dir lib --copy-files",
    "build:watch": "npm run clean && node ./node_modules/babel-cli/bin/babel src/ --out-dir lib --watch --copy-files",
    "lint": "eslint src/",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "test:file": "npm test -- --grep",
    "test:file:watch": "npm test -- --watch --grep",
    "docs": "esdoc -c ./esdoc.json",
    "docs:watch": "esdoc --watch -c ./esdoc.json",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel-register --recursive",
    "cover": "npm run coverage",
    "dev-publish": "npm publish --tag=dev",
    "prepublish": "npm run lint && npm run test && npm run build",
    "prepush": "npm run lint"
  },
  "author": "@stackscz",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-tcomb": "^0.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-webpack-alias": "^1.1.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bootstrap": "^3.3.6",
    "cross-env": "^1.0.8",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^2.13.1",
    "eslint-config-stacks": "0.0.3",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-flowtype": "^2.3.0",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.1",
    "husky": "^0.11.6",
    "istanbul": "^1.1.0-alpha.1",
    "json-loader": "^0.5.4",
    "loopback-datasource-juggler": "^2.47.0",
    "marked": "^0.3.5",
    "mocha": "^2.5.3",
    "pouchdb": "^5.3.2",
    "prismjs": "^1.4.1",
    "re-app-builder": "^0.5.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-immutable-state-invariant": "^1.2.3",
    "relational-pouch": "^1.4.2",
    "rimraf": "^2.5.3",
    "webpack-node-externals": "^1.0.0"
  },
  "dependencies": {
    "anchorate": "^1.1.0",
    "gonzales-pe": "^3.4.4",
    "history": "^1.17.0",
    "json-schema-defaults": "^0.2.0",
    "json-schema-deref-local": "^0.2.1",
    "json-schema-ref-parser": "^3.1.2",
    "jsonschema": "^1.1.0",
    "jsx-to-string": "^0.2.16",
    "react-json-inspector": "^7.0.0",
    "react-modal2": "^3.0.2",
    "react-portal": "^2.1.3",
    "redux-batched-subscribe": "^0.1.5",
    "redux-logger": "^2.6.1",
    "scroll-behavior": "^0.8.1"
  },
  "peerDependencies": {
    "classnames": "^2.2.3",
    "denormalizr": "0.2.0",
    "dot-object": "^1.4.1",
    "fbjs": "^0.8.2",
    "invariant": "^2.2.1",
    "lodash": "^4.5.0",
    "moment": "^2.12.0",
    "normalizr": "^2.1.0",
    "object-hash": "^1.1.2",
    "qs": "^6.1.0",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.1.0",
    "react-dom": "^15.0.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.5.1",
    "redux-form": "^6.0.2",
    "redux-saga": "^0.10.3",
    "seamless-immutable": "^6.1.0",
    "tcomb": "^3.2.2",
    "tcomb-validation": "^3.0.0"
  },
  "npmName": "re-app"
}
